{
    "compilerOptions": {
        "target": "es2019",
        "module": "commonjs",
        "lib": [
            "es2019"
        ],
        "rootDir": "./nodes",
        "outDir": "./dist/nodes",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "nodes/**/*"
    ]
}